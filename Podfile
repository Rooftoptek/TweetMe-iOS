source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/Rooftoptek/Cocoapods.git'

# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TweetMe' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for TweetMe
    pod 'Rooftop', :git => 'https://github.com/Rooftoptek/Rooftop-SDK-xOS.git', :tag => '0.5.0'
    pod 'RooftopFacebookUtils', :git => 'https://github.com/Rooftoptek/RooftopFacebookUtils-xOS.git', :tag => '0.5.0'
    pod 'MRProgress'
end
